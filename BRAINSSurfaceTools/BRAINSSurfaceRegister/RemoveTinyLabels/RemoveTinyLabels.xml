<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>
    BRAINS.Surface
  </category>
  <title>
    BRAINS Remove Labels
  </title>
  <description>
      This program removes each label in LabelList and replaces it with the label that has maximum points around the removal label patch. It is used to clean up labels that only associated with few vertices on the surface. ASSUME: inputSurfaceFile does not have more than 50 labels on it.
  </description>
  <version>4.8.0</version>
  <documentation-url>http://www.nitrc.org/plugins/mwiki/index.php/brains:BRAINSSurfaceRegister</documentation-url>
  <license>https://www.nitrc.org/svn/brains/BuildScripts/trunk/License.txt</license>
  <contributor>Wen Li and Vincent Magnotta</contributor>
  <acknowledgements>Funding for this work was provided by NIH/NINDS award NS050568</acknowledgements>

  <parameters>
    <label>Input Parameters</label>

    <file>
      <name>inputSurfaceFile</name>
      <longflag>inputSurfaceFile</longflag>
      <description>Input Surface Filename</description>
      <label>Input Surface</label>
      <channel>input</channel>
    </file>

  </parameters>

    <parameters>
        <label>Label Parameters</label>

    <integer-vector>
        <name>labelList</name>
        <longflag>labelList</longflag>
        <description>label List</description>
        <label>label List</label>
    </integer-vector>

  </parameters>

  <parameters>
    <label>Output Parameters</label>

    <file>
      <name>outputSurfaceFile</name>
      <longflag>outputSurfaceFile</longflag>
      <description>Output Surface Filename</description>
      <label>Output Surface</label>
      <channel>output</channel>
    </file>

  </parameters>

</executable>
